{
  "name": "ey-next-ts-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -e base,dev next dev",
    "local": "env-cmd -e base,local next dev",
    "qa": "env-cmd -e base,qa next dev",
    "build": "env-cmd --no-override -e base,prod next build",
    "build:local": "env-cmd -e base,local next build",
    "build:dev": "env-cmd -e base,dev next build",
    "build:qa": "env-cmd -e base,qa next build",
    "start": "env-cmd --no-override -e base,prod next start",
    "start:local": "env-cmd -e base,local next dev",
    "start:dev": "env-cmd -e base,dev next dev",
    "start:qa": "env-cmd -e base,qa next dev",
    "lint": "next lint"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.16",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "typescript": "4.3.5"
  }
}
